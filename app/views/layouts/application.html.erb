<!DOCTYPE html>
<html>
<head>
  <title>sambragg.co.uk | <%= @head_title %></title>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <%= auto_discovery_link_tag(:rss, rss_link) %>

<script type="text/javascript"
    src="http://maps.google.com/maps/api/js?sensor=false">
</script>

</head>
<body onload="initialize()">
	<div class="container">
		<header>
			<%= link_to image_tag("sambragg_logo.png", :alt => "Sam Bragg"), root_path %>		
			
			
		</header>
		<nav class="round">
			<ul>
				<li><%= link_to "News", root_path %></li>
				<li><%= link_to "About", about_path %></li>
   				<li><%= link_to "Gigs", gigs_path %></li>
				<li><%= link_to "Music", '#' %></li>
				<li><%= link_to "Photos", '#' %></li>
				<li><%= link_to "Contact", '#' %></li>
			</ul>
		</nav>
		<section class="round">
			<%= yield %>
		</section>
		<%#= render 'layouts/footer' %>
			
	</div>


</body>
</html>
